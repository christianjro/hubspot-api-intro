doctype html
html
    head 
        title = `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body 
        h1 Contacts 
        .cards 
            each contact in data 
                .card 
                    h2.card_name #{contact.properties.firstname} #{contact.properties.lastname}
                    p.card_email #{contact.properties.email}
                